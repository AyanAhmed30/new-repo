{
    "version": 2,
    "builds": [
      {
        "src": "djangoGEE/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "runtime": "python3.12",
          "maxLambdaSize": "15mb"
        }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "djangoGEE/wsgi.py"
      }
    ]
}

  